public class P10 {

	public static void main(String[] args) throws IOException {
		// TODO Auto-generated method stub
		List builders = Arrays.asList(
				 new ProcessBuilder("echo","hola"),
				  new ProcessBuilder("finstr","hola"));
		Process pipeline = (Process) ProcessBuilder.startPipeline(builders);
		
	}

}
